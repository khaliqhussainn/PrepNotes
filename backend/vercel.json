{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate && npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  }
}