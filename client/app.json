{
  "expo": {
    "name": "HamdardDocs",
    "slug": "hamdarddocs",
    "version": "1.0.0",
    "scheme": "hamdarddocs",
    "platforms": [
      "ios",
      "android"
    ],
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-document-picker",
        {
          "permissions": [
            "READ_EXTERNAL_STORAGE",
            "WRITE_EXTERNAL_STORAGE"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2ac4e880-5752-4b19-9185-4a7188fc149a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.HamdardDocs.app"
    },
    "android": {
      "package": "com.HamdardDocs.app",
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      }
    },
    "owner": "khaliqhussainn"
  }
}
